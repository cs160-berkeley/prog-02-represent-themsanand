<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cs169_au.represent">

    <!-- Required to act as a custom watch face. -->
    <uses-permission android:name="com.google.android.permission.PROVIDE_BACKGROUND" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Represent"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Congressional"
            android:label="@string/title_activity_congressional"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Detailed"
            android:label="@string/title_activity_detailed"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailChinton"
            android:label="@string/title_activity_detail_chinton"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailRump"
            android:label="@string/title_activity_detail_rump"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
